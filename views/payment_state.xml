<odoo>
    <record id="payment_state_view_tree" model="ir.ui.view">
        <field name="name">payment.state.view.tree</field>
        <field name="model">payment.state</field>
        <field name="arch" type="xml">
            <tree string="Payment state" editable="top">
                    <field name="id"/>
                    <field name="name" string="state"/>                  
            </tree>
        </field>
    </record>
</odoo>