<odoo>
    <record id="apartment_management_view_tree" model="ir.ui.view">
        <field name="name">apartment.management.view.tree</field>
        <field name="model">apartment.management</field>
        <field name="arch" type="xml">
            <tree string="Apartment management">
                    <field name="id"/>
                    <field name="name" string="Appartement"/>
                    <field name="homeowner"/>
                    <field name="share_of_ownership"/>
                    <field name="montlhy_payment" widget="monetary"/>
                    <field name="payment_submission"/>
                    <field name="currency_id" invisible="1"/>
                    <field name="garage"/>
                    <field name="parking_space"/>                   
            </tree>
        </field>
    </record>

    

</odoo>